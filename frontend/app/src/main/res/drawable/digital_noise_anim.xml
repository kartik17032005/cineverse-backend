<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Noise pattern 1 -->
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:type="linear"
                android:angle="45"
                android:startColor="#00FFFFFF"
                android:centerColor="#1000FFFF"
                android:endColor="#00FFFFFF" />
            <corners android:radius="25dp" />
        </shape>
    </item>
    <!-- Noise pattern 2 -->
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:type="linear"
                android:angle="135"
                android:startColor="#00FFFFFF"
                android:centerColor="#0800FFFF"
                android:endColor="#00FFFFFF" />
            <corners android:radius="25dp" />
        </shape>
    </item>
    <!-- Scan interference -->
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:type="linear"
                android:angle="0"
                android:startColor="#00FFFFFF"
                android:centerColor="#0500FFFF"
                android:endColor="#00FFFFFF" />
            <corners android:radius="25dp" />
        </shape>
    </item>
</layer-list>
